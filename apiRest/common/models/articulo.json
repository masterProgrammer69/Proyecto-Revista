  "options": {
    "validateUpsert": true
  },
  "properties": {
    "titulo": {
      "type": "string",
      "required": true
    },
    "abstract": {
      "type": "string",
      "required": true
    },
    "palabrasClave": {
      "type": "string",
      "required": true
    },
    "fecha": {
      "type": "date"
    },
    "estado": {
      "type": "string",
      "required": true,
      "default": "enviado"
    },
    "idAutor": {
      "type": "string"
    },
    "idEdicion": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
